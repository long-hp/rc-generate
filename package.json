{
  "name": "react-generate",
  "version": "0.1.0",
  "description": "nodejs",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": "https://github.com/wiloke1/react-generate",
  "bugs": {
    "url": "https://github.com/wiloke1/react-generate/issues"
  },
  "homepage": "https://github.com/wiloke1/react-generate#readme",
  "author": "wiloke1",
  "license": "MIT",
  "keywords": [],
  "bin": {
    "react-generate": "./bin/react-generate"
  },
  "scripts": {
    "build:ts": "ttsc && babel src --out-dir dist",
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "start": "export NODE_ENV=dev && rm -rf dist && yarn build:ts && node ./dist/index.js",
    "start:ts": "ts-node ./src/index.ts",
    "start:watch": "nodemon --inspect",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix"
  },
  "react-generate": {
    "rootPath": "src",
    "typescript": true,
    "react-native": false
  },
  "_moduleAliases": {
    "components": "src/components",
    "routes": "src/routes",
    "types": "src/types",
    "utils": "src/utils",
    "config": "src/config"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start:ts",
    "ext": "ts"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "^0.0.42",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^9.0.3",
    "@types/helmet": "^0.0.48",
    "@types/mongoose": "^5.7.36",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.5",
    "@types/ramda": "^0.27.28",
    "@types/slug": "^0.9.1",
    "@types/socket.io": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "ava": "^3.12.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "codecov": "^3.5.0",
    "commander": "^6.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "^1.4.0",
    "fs-extra": "^9.0.1",
    "gh-pages": "^3.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.2",
    "reload": "^3.1.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
